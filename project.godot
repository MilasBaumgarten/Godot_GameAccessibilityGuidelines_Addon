; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "Input_Settings",
"language": "GDScript",
"path": "res://addons/keybindings/resources/input_settings.gd"
} ]
_global_script_class_icons={
"Input_Settings": ""
}

[application]

config/name="Godot Game Accessibility Guidelines Addon"
run/main_scene="res://scenes/main_menu.tscn"
config/icon="res://icon.png"

[autoload]

KeyPersistence="*res://addons/keybindings/key_persistence.gd"
Subtitles="*res://addons/subtitles/subtitles.gd"

[editor_plugins]

enabled=PoolStringArray( "res://addons/keybindings/plugin.cfg", "res://addons/subtitles/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input]

ui_end={
"deadzone": 0.5,
"events": [  ]
}

[locale]

translations=PoolStringArray(  )

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
